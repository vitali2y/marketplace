{
  "name": "marketplace",
  "version": "0.0.2",
  "description": "Marketplace",
  "scripts": {
    "postinstall": "./scripts/postinstall.sh && cd ../marketplace_client && npm install && cd -; exit $?",
    "jsbin": "ps -fewwww | grep jsbin | awk '{ print $2 }' | xargs kill -TERM; echo 'starting jsbin on http://localhost:3000'; jsbin &> /dev/null &",
    "test": "echo \"Error: no test specified\" && exit 1",
    "alice": "cd ../marketplace_client; npm run alice; cd -",
    "bob": "cd ../marketplace_client; npm run bob; cd -",
    "james": "cd ../marketplace_client; npm run james; cd -",
    "tom": "cd ../marketplace_client; npm run tom; cd -",
    "server": "NODE_ENV=development LOGGY_STACKS=1 ./node_modules/.bin/brunch w -s",
    "dev": "./node_modules/.bin/concurrently --success last --names 'SRV,BOB,JAMES,TOM,ALICE' --prefix '{name}-{time}' -t HH:mm:ss --prefix-colors cyan,magenta,green,yellow,blue 'npm run server' 'npm run bob' 'npm run james' 'npm run tom' 'npm run alice'",
    "open": "hs -p 3000 ./public",
    "backup": "F=$npm_package_name$(date +%y%m%d-%H%M).tbz; tar cvfjh ../$F . --exclude=./node_modules --exclude=./marketplace_client/node_modules --exclude=./build --exclude=./marketplace_client/build --exclude=./.git --exclude=./marketplace_client/.git --exclude=./marketplace_client/dist/*/store_*; echo 'ready: ../'$F",
    "release_bin": "./node_modules/.bin/brunch b; ./scripts/release_bin.sh; if [ $? -ne 0 ]; then echo 'failed!!!'; exit 9; fi; echo 'server released'; cd ./marketplace_client; ./scripts/release_bin.sh; if [ $? -ne 0 ]; then echo 'failed!!!'; exit 9; fi; cd -"
  },
  "keywords": [
    "marketplace",
    "p2p",
    "PoC"
  ],
  "author": "Vi+ <vitaliyy@gmail.com>",
  "dependencies": {
    "crypto": "^1.0.1",
    "lowdb": "^1.0.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "vue": "^2.5.13",
    "vue-awesome": "^2.3.4"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "babel-brunch": "^6.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "brunch": "^2.10.12",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^1.12.7",
    "coffee-script-brunch": "^2.10.2",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "html2jade": "^0.8.6",
    "jade": "^1.11.0",
    "jade-brunch": "^2.8.0",
    "jade-loader": "^0.8.0",
    "js2coffee": "^2.2.0",
    "pkg": "^4.3.0",
    "spectre.css": "^0.5.0",
    "static-jade-brunch": "^2.7.0",
    "stylus": "^0.54.5",
    "stylus-brunch": "^2.10.0",
    "stylus-loader": "^3.0.1",
    "uglify-js-brunch": "^2.10.0",
    "vue-brunch": "^2.0.1",
    "vueify": "^9.4.1",
    "vueify-extract-css": "^0.2.0"
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      "public/**/*"
    ]
  }
}
